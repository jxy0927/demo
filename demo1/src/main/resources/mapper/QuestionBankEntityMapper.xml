<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.QuestionBankEntityMapper">
<!--查找所有项目信息-->
<select id="queryQuestionList" parameterType="java.lang.String" resultType="com.example.demo.dao.entity.QuestionBankEntity">
        SELECT *
        FROM questionbank
        WHERE problemid = #{problemId,jdbcType=VARCHAR}
    </select>


</mapper>